.container .course {
  width: 100%;
  height: 170px;
}
.container .course .video {
  position: relative;
}
.container .course .video .btn-play {
  position: absolute;
  width: 138px;
  height: 32px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.container .course .video image {
  width: 100%;
  height: 170px;
}
.container .pro-box {
  text-align: center;
  padding: 20px 0;
}
.container .pro-box .title {
  display: inline-block;
  font-size: 16px;
  height: 35px;
}
.container .pro-box .title::before {
  content: " ";
  display: inline-block;
  width: 38px;
  height: 14px;
  margin-right: 5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.container .pro-box .title::after {
  content: " ";
  display: inline-block;
  width: 38px;
  height: 14px;
  margin-left: 5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.container .pro-box .nav {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.container .pro-box .nav .nav-btn {
  width: 70px;
  height: 70px;
  border-radius: 5px;
  box-shadow: gainsboro 0px 0px 5px;
  text-align: center;
  font-size: 12px;
  padding-top: 5px;
  box-sizing: border-box;
}
.container .pro-box .nav .nav-btn .iconfont {
  font-size: 36px;
}
.container .couse-class .class-pane {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.container .content-img {
  width: 100%;
}
.container .content-img image {
  width: 100%;
  height: 654rpx;
}

