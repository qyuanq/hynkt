.van-loading {
  display: -webkit-box !important;
  display: -webkit-flex !important;
  display: flex !important;
  margin: 10px 0;
}
.container .custom {
  z-index: 999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
}
.container .product {
  padding: 0 30rpx;
}
.container .van-search {
  width: 565rpx;
  padding: 4px 12px;
}
.container .van-tree-select__content {
  background-color: #eee !important;
}
.container .navContent {
  height: 100%;
  position: relative;
}
.container .navContent .childNav {
  padding: 12.5px 10px;
}
.container .navContent .childActive {
  background: #ddd;
}
.container .navContent .checkbox {
  width: 300rpx;
  height: 100%;
  background: #fff;
  position: absolute;
  right: 0;
  top: 0;
}
.container .navContent .checkbox .checkText {
  padding: 12.5px 10px;
}
.container .indexBar {
  width: 100%;
  margin-top: 110px;
  background: #fff;
  overflow: hidden;
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
}
.container .scroll {
  height: 100%;
}

