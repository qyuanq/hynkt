<view class="container"><van-nav-bar vue-id="2796fb0a-1" title="答题结果" data-com-type="wx" bind:__l="__l"></van-nav-bar><block wx:if="{{isType==='test'||'collection'}}"><view class="result-box"><view class="result">{{correct+"%"}}</view><view class="name">{{resultName}}</view></view></block><block wx:else><block wx:if="{{isType==='simulation'}}"><view class="result-box"><view class="result-title">{{testName}}</view><view class="result-text"><view class="text-item"><icon class="iconfont my-icon-time"></icon><text>{{"考试时间："+userTime}}</text></view><view class="text-item"><icon class="iconfont my-icon-score"></icon><text>{{"考试得分："+score}}</text></view></view></view></block></block><test-result vue-id="2796fb0a-2" result="{{result}}" bind:__l="__l"></test-result><view class="btns"><block wx:if="{{isType==='test'}}"><button data-event-opts="{{[['tap',[['tryAgain',['$event']]]]]}}" class="btn try-again" bindtap="__e">再测一次</button></block><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="btn confirm" bindtap="__e">确认</button></view></view>