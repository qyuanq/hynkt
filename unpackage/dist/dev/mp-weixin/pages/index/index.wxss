.container {
  font-size: 14px;
  line-height: 24px;
  background: #F6F7F8;
}
.container .top {
  width: 100%;
  height: 44px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  vertical-align: middle;
}
