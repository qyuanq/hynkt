}
.container .top .classcs .cource {
  width: 40px;
  height: 20px;
  font-size: 16px;
  background: #FD9801;
  color: #fff;
  line-height: 20px;
  text-align: center;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  margin-left: 10px;
