<view class="container"><view class="course"><block wx:if="{{videoShow}}"><view class="video"><block wx:if="{{videoPath}}"><video src="{{videoPath}}" enable-auto-rotation="true" autoplay="true" controls="{{true}}" initial-time="{{current}}" data-event-opts="{{[['ended',[['videoEnded',['$event']]]],['timeupdate',[['setCurrentTime',['$event']]]]]}}" bindended="__e" bindtimeupdate="__e"></video></block></view></block><block wx:else><view class="videoEnd"><view class="content"><view class="text">该视频已完成</view><view class="btns"><button data-event-opts="{{[['tap',[['replay',['$event']]]]]}}" class="btn" bindtap="__e"><van-icon class="iconfont" vue-id="03b2b160-1" class-prefix="my-icon" name="chongbo" data-com-type="wx" bind:__l="__l"></van-icon>重学一遍</button><block wx:if="{{btnShow}}"><button data-event-opts="{{[['tap',[['nextVideo',['$event']]]]]}}" class="btn" bindtap="__e">观看下一节<van-icon class="iconfont" vue-id="03b2b160-2" class-prefix="my-icon" name="xiayijie" data-com-type="wx" bind:__l="__l"></van-icon></button></block></view></view></view></block><view class="info"><view class="title">{{title}}</view><view class="nav"><view class="looked"><text class="text">{{"已看 "+proCount}}</text></view><view data-event-opts="{{[['tap',[['onCollect',['$event']]]]]}}" class="{{[collect_state?'collect-check':'','collect']}}" bindtap="__e"><icon class="iconfont my-icon-shoucang"></icon>收藏</view><view data-event-opts="{{[['tap',[['onCache',['$event']]]]]}}" class="cache" bindtap="__e"><icon class="iconfont my-icon-huancun"></icon>缓存</view><view data-event-opts="{{[['tap',[['onAnswer',['$event']]]]]}}" class="answer" bindtap="__e"><icon class="iconfont my-icon-dayi"></icon>答疑</view></view></view></view><view class="learn-box"><view class="catalog">目录</view><view class="learn-list"><van-collapse vue-id="03b2b160-3" value="{{activeNames}}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{goodVideos.content}}" wx:for-item="item" wx:for-index="index" wx:key="section"><van-collapse-item vue-id="{{('03b2b160-4-'+index)+','+('03b2b160-3')}}" title="{{item.section}}" value="{{(arrs[index].progress?arrs[index].progress:0)+'/'+item.value.length}}" name="{{index+1}}" title-class="{{sec_selected===index?'active':' '}}" value-class="{{sec_selected===index?'active':' '}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{item.value}}" wx:for-item="video" wx:for-index="idx" wx:key="id"><view data-event-opts="{{[['tap',[['changeVideo',['$0',index,idx],[[['goodVideos.content','section',item.section],['value','id',video.id,'path']]]]]]]}}" class="{{['cell',vid_selected===idx&&sec_selected===index?'active':' ']}}" bindtap="__e"><icon class="iconfont my-icon-video"></icon>{{''+(idx+1+'. '+video.name)+''}}<icon class="iconfont my-icon-xiazai"></icon></view></block></van-collapse-item></block></van-collapse></view></view></view>