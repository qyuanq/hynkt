.container {
  width: 100%;
  box-sizing: border-box;
}
.container .serial {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
.container .serial .headlines {
  width: 353rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
  line-height: 36rpx;
  text-indent: 20rpx;
  color: #fff;
}
.container .serial .count {
  color: #41A5FD;
}
.container .title {
  padding: 0 30rpx;
  margin: 40rpx 0;
}
.container .options {
  margin-top: 40rpx;
}
.container .options .cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 100%;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #ebedf0;
  box-sizing: border-box;
}
.container .options .cell:first-child {
  border-top: 1px solid #ebedf0;
}
.container .options .cell .iconfont {
  font-size: 48rpx !important;
  float: left;
}
.container .options .cell .text {
  width: 570rpx;
  margin-left: 20rpx;
  margin-top: 6rpx;
  float: left;
}
.container .options .cell .textActive {
  color: #41A5FD;
}
.container .look-answer {
  padding: 0 20rpx;
  font-size: 14px;
}
.container .look-answer .top {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #ccc;
}
.container .look-answer .top .correct-answer {
  color: #41A5FD;
}
.container .look-answer .top .error-answer {
  color: #DC5044;
}
.container .look-answer .parsing .title {
  font-size: 14px;
  color: #333333;
  cont-weight: 700;
}
.container .look-answer .parsing .content {
  padding: 20rpx 10rpx;
  width: 710rpx;
  margin: 20rpx auto 0;
  border: 1px dashed #c6c6c6;
  background: #F6F7F8;
  box-sizing: border-box;
}
.container .sheet .van-popup {
  padding-bottom: 30rpx;
  max-height: 300px !important;
}
.container .sheet .content .dtk-title {
  margin: 0 35rpx;
}
.container .sheet .content .options .option {
  width: 80rpx;
  height: 80rpx;
  margin: 17.5rpx 35rpx;
  border-radius: 50%;
  line-height: 80rpx;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background: #E2E2E2;
}
.container .sheet .content .options .do-mark {
  background: #3fbf72;
}
.container .sheet .content .options .not-rate {
  background: #ff5500;
}
.container .sheet .btn {
  width: 200rpx;
  margin: 20rpx auto;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background: #41A5FD;
  border-radius: 40rpx;
}
.container .tabbar {
  width: 100%;
  height: 50px;
  position: fixed;
  left: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  text-align: center;
  border-top: 1px solid #ccc;
  z-index: 1;
}
.container .tabbar .active {
  color: #41A5FD !important;
}
.container .tabbar .iconfont {
  font-size: 18px;
}
.container .tabbar .tabbar-name {
  font-size: 12px;
  color: #646566;
}
.container .suspend-button {
  position: fixed;
  top: 50%;
  right: 0;
  width: 80rpx;
  height: 160rpx;
  background: #41A5FD;
  opacity: .4;
  border-radius: 80rpx 0 0 80rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  font-size: 14px;
  color: #fff;
}
.container .suspend-button .text {
  position: absolute;
  top: 35rpx;
  left: 20rpx;
  -webkit-writing-mode: tb-rl;
          writing-mode: tb-rl;
}
.container .suspend-button .complete {
  top: 50rpx !important;
}
.container .suspend-button .jt {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 0;
}

